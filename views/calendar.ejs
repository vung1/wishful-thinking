<% layout('layouts/boilerplate')%>
<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<!------ Include the above in your HEAD tag ---------->
	
	<div class="container">
		<div class="d-flex justify-content-between">
			<h2 class="mb-4"><u>Dates</u></h2>
			<a href="/addDate"><button type="button" class="btn btn-success btn-sm">Add Date</button></a>
		</div>

		<div class="list-group">
			<% for (let date of data.dates ){%>
				<div class="list-group-item list-group-item-action flex-column align-items-start mb-3" id="id01">
					<div class="d-flex w-100 justify-content-between">
					  <h5 class="mb-1"><%= date.name %></h5>
					  <%= date.date %>
					</div>
				<p class="mb-1"><%= date.description %></p>
		  </div>
			<% } %>
			</div>
		  
	</div>
